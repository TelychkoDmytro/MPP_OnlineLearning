<h1>Students#edit</h1>
<p>Find me in app/views/students/edit.html.erb</p>

<%= form_with(model: @student, local: true) do |form| %>

	<div class="field">
	    <%= form.label :group %>
	    <%= form.collection_select :group_id, Group.all, :id, :name, {include_blank: "Без Групи" }, prompt: true %>
	</div>
	
	<div class="actions">
		<%= form.submit "Update" %>
	</div>

<% end %>

<%= link_to 'Back', student_path %>
